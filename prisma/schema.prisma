generator client {
  provider = "prisma-client-js"
  //previewFeatures = ["mongodb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Edificios {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  //recursos Recursos[]
 // recurso Recursos
  //recursoId String @db.ObjectId
  nivel Int
}

model Recursos{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  //recursoNecesario String
 // recurso Recursos @relation(fields: [recursoId], references: [id])

}

